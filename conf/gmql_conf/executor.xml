<gmqlProperties xmlns="http://genomic.elet.polimi.it/impl">
  <property name="SPARK_HOME">/usr/local/spark/</property>
  <property name="CLI_JAR_NAME">GMQL-Cli-1.0-SNAPSHOT-jar-with-dependencies.jar</property>
  <property name="CLI_CLASS">it.polimi.genomics.cli.GMQLExecuteCommand</property>
  <property name="LAUNCHER_MODE">CLUSTER</property>
  <property name="LIB_DIR_LOCAL">conf/gmql_lib/</property>
  <property name="LIB_DIR_HDFS">/user/repository/lib/</property>

  <property spark-property="spark.cores.max" user-category="GUEST" name="SPARK_CUSTOM">6</property>
  <property spark-property="spark.cores.max" user-category="BASIC" name="SPARK_CUSTOM">12</property>
  <property spark-property="spark.cores.max" user-category="PRO" name="SPARK_CUSTOM">15</property>
  <property spark-property="spark.cores.max" user-category="ADMIN" name="SPARK_CUSTOM">18</property>
  <property spark-property="spark.cores.max" user-category="PUBLIC" name="SPARK_CUSTOM">18</property>

</gmqlProperties>